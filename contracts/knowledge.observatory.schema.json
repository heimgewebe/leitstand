{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Knowledge Observatory Artifact",
  "type": "object",
  "required": [
    "generated_at",
    "source",
    "topics"
  ],
  "properties": {
    "generated_at": {
      "type": "string",
      "format": "date-time"
    },
    "source": {
      "type": "string"
    },
    "topics": {
      "type": "array"
    }
  }
}
